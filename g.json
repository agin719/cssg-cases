{
  "docExtension": ".md",
  "compileDist": "dist",

  "docSetRemoteGitUrl": "https://github.com/tencentyun/qcloud-documents.git",
  "sdkDocSetRelativePath": "/product/存储与CDN/对象存储 4.0/SDK文档",

  "macro4doc": {
    "secretId": "COS_SECRETID",
    "secretKey": "COS_SECRETKEY",

    "appId": "1250000000",
    "uin": "100000000001",
    "region": "COS_REGION",
    "bucket": "examplebucket-",
    "copySourceBucket": "sourcebucket-",
    "replicationDestBucket":"destinationbucket-",
    "object": "exampleobject",

    "eTag": "exampleETag",
    "uploadId": "exampleUploadId"
  },
  "macro4test": {
    "appId": "1253653367",
    "uin": "1278687956",
    "region": "ap-guangzhou",
    "bucket": "bucket-cssg-test-${config.language}-",
    "copySourceBucket":"bucket-cssg-source-",
    "replicationDestBucket":"bucket-cssg-assist-",
    "object": "object4${config.language}"
  },

  "globalInitNamePrefix": "global-init",
  "globalHeaderName": "global-header",

  "testGroup": {
    "bucket": {
      "isBucketTest": true,
      "setup": ["put-bucket"],
      "teardown": ["delete-bucket"],
      
      "bucket-ACL": [
        "get-bucket",
        "head-bucket",
        "put-bucket-acl",
        "get-bucket-acl"
      ],
      "bucket-policy": [
        "put-bucket-policy",
        "get-bucket-policy",
        "delete-bucket-policy"
      ],
      "bucket-CORS": [
        "put-bucket-cors",
        "get-bucket-cors",
        "option-object",
        "delete-bucket-cors"
      ],
      "bucket-lifecycle": [
        "put-bucket-lifecycle",
        "get-bucket-lifecycle",
        "delete-bucket-lifecycle"
      ],
      "bucket-replication-and-versioning": [
        "put-bucket-versioning",
        "get-bucket-versioning",
        "put-bucket-replication",
        "get-bucket-replication",
        "delete-bucket-replication"
      ],
      "bucket-tagging": [
        "put-bucket-tagging",
        "get-bucket-tagging",
        "delete-bucket-tagging"
      ],
      "bucket-logging": [
        "put-bucket-logging",
        "get-bucket-logging"
      ],
      "bucket-domain": [
        "put-bucket-domain",
        "get-bucket-domain"
      ],
      "bucket-website": [
        "put-bucket-website",
        "get-bucket-website",
        "delete-bucket-website"
      ],
      "bucket-inventory": [
        "put-bucket-inventory",
        "get-bucket-inventory",
        "delete-bucket-inventory"
      ]
    },

    "object": {
      "setup": ["put-bucket"],
      "teardown": ["delete-object", "delete-bucket"],
      "isObjectTest": true,

      "object-metadata": [
        "put-object",
        "put-object-acl",
        "get-object-acl",
        "head-object",
        "get-object",
        "get-presign-download-url",
        "get-presign-upload-url",
        "delete-object",
        "delete-multi-object",
        "post-object",
        "restore-object"
      ],

      "object-multi-upload": [
        "init-multi-upload",
        "list-multi-upload",
        "upload-part",
        "list-parts",
        "complete-multi-upload"
      ],

      "object-abort-multi-upload": [
        "init-multi-upload",
        "upload-part",
        "abort-multi-upload"
      ],

      "object-transfer": [
        "transfer-upload-object",
        "transfer-download-object",
        "transfer-copy-object",
        "batch-upload-objects"
      ],

      "object-copy": [
        "copy-object",
        "init-multi-upload",
        "upload-part-copy",
        "complete-multi-upload"
      ]
    },

    "service": {
      "get-service": [
        "get-service",
        "get-regional-service",
        "get-authorization"
      ]
    }
  },

  "testResultFree": [
    "put-bucket",
    "delete-bucket",
    "restore-object"
  ]
}