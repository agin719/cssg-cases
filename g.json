{
  "docExtension": ".md",
  "compileDist": "dist",

  "docSetRemoteGitUrl": "https://github.com/tencentyun/qcloud-documents.git",
  "sdkDocSetRelativePath": "/product/存储与CDN/对象存储 4.0/SDK文档",

  "macro4doc": {
    "secretId": "COS_SECRETID",
    "secretKey": "COS_SECRETKEY",

    "appId": "1250000000",
    "uin": "100000000001",
    "region": "COS_REGION",
    "bucket": "examplebucket-",
    "copySourceBucket": "sourcebucket-",
    "replicationDestBucket":"destinationbucket-",
    "object": "exampleobject",

    "eTag": "exampleETag",
    "uploadId": "exampleUploadId"
  },
  "macro4test": {
    "appId": "1253653367",
    "uin": "1278687956",
    "region": "ap-guangzhou",
    "bucket": "bucket-cssg-test-${config.language}-",
    "copySourceBucket":"bucket-cssg-source-",
    "replicationDestBucket":"bucket-cssg-assist-",
    "object": "object4${config.language}"
  },

  "globalInitNamePrefix": "global-init",
  "globalHeaderName": "global-header",

  "methodsCategory": ["setup", "steps", "teardown"],

  "testGroup": {
    "bucket-ACL": {
      "setup": ["put-bucket"],
      "steps": [
        "get-bucket",
        "head-bucket",
        "put-bucket-acl",
        "get-bucket-acl"
      ],
      "teardown": [
        "delete-bucket"
      ]
    },
    "bucket-CORS": {
      "setup": ["put-bucket"],
      "steps": [
        "put-bucket-cors",
        "get-bucket-cors",
        "option-object",
        "delete-bucket-cors"
      ],
      "teardown": [
        "delete-bucket"
      ]
    },
    "bucket-lifecycle": {
      "setup": ["put-bucket"],
      "steps": [
        "put-bucket-lifecycle",
        "get-bucket-lifecycle",
        "delete-bucket-lifecycle"
      ],
      "teardown": [
        "delete-bucket"
      ]
    },
    "bucket-replication-and-versioning": {
      "setup": ["put-bucket"],
      "steps": [
        "put-bucket-versioning",
        "get-bucket-versioning",
        "put-bucket-replication",
        "get-bucket-replication",
        "delete-bucket-replication"
      ],
      "teardown": [
        "delete-bucket"
      ]
    },
    "object": {
      "setup": ["put-bucket"],
      "steps": [
        "put-object",
        "put-object-acl",
        "get-object-acl",
        "head-object",
        "get-object",
        "delete-object",
        "delete-multi-object",
        "post-object",
        "restore-object",
        "get-presign-download-url",
        "get-presign-upload-url"
      ],
      "teardown": ["delete-object", "delete-bucket"]
    },
    "object-transfer": {
      "setup": ["put-bucket"],
      "steps": [
        "init-multi-upload",
        "list-multi-upload",
        "upload-part",
        "list-parts",
        "complete-multi-upload",
  
        "init-multi-upload",
        "upload-part",
        "abort-multi-upload",

        "transfer-upload-object",
        "transfer-download-object",
        "transfer-copy-object"
      ],
      "teardown": ["delete-object", "delete-bucket"],
      "isMultiUpload": true
    },
    "object-copy": {
      "setup": ["put-bucket"],
      "steps": [
        "copy-object",
        "init-multi-upload",
        "upload-part-copy",
        "complete-multi-upload"
      ],
      "teardown": ["delete-object", "delete-bucket"],
      "isMultiUpload": true
    },
    "get-service": {
      "steps": [
        "get-service"
      ]
    }
  },

  "testResultFree": [
    "put-bucket",
    "delete-bucket",
    "restore-object"
  ]
}