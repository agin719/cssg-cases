{
  "language": "android",
  "sourcesRoot": "./project/app/src/main/java/com/tencent/qcloud/cosxml/cssg/",
  "sourceExtension": ".java",
  "docsRoot": "docs",
  "docExtension": ".md",

  "testcaseTemplate": "TestCase.t",

  "ignoreExpressionInDoc": [
    "credentialProvider = new ShortTimeCredentialProvider.* for ut",
    "assertError().*",
    "this.uploadId = uploadId;",
    "uploadId = this.uploadId;",
    "this.part1Etag = eTag;",
    "etag = this.part1Etag;"
  ],

  "compileDist": "project/app/src/androidTest/java/com/tencent/qcloud/cosxml/cssg/",

  "macro4doc": {
  },
  "macro4test": {
    "secretId": "BuildConfig.COS_SECRET_ID",
    "secretKey": "BuildConfig.COS_SECRET_KEY"
  },
  "testGroup": {
    "init-custom-provider": {
      "defines": [
        "global-init-custom-provider"
      ]
    }
  }
}
