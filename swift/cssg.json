{
  "language": "swift",
  "sourcesRoot": "COS_Swift_TestTests/",
  "sourceExtension": ".swift",

  "docRoot": "iOS SDK/",

  "testcaseTemplate": "TestCase.t",

  "snippetNameCommonPrefix": "swift-",

  "ignoreExpressionInDoc": [
    "let\\s+exception\\s+=\\s+XCTestExpectation\\.init\\(description:\\s*.+\\);\\r\\n",
    "exception\\s*\\.fulfill\\(\\);\\r\\n",
    "self\\.wait\\(for:\\s*\\[exception\\],\\s*timeout:\\s*.+\\);",
    "XCTAssertNotNil\\(.+\\);",
    "XCTAssertNil\\(.+\\);"
  ],

  "macro4doc": {
  },
  "macro4test": {
    "secretId": "SecretStorage.shared.secretID",
    "secretKey": "SecretStorage.shared.secretID.secretKey"
  },
  "testGroup": {
    "global-init": {
      "defines": [
        "global-init",
        "global-init-fence-queue",
        "global-init-signature",
        "global-init-signature-sts"
      ],
      "isDefine": true
    }
  }
}
